#Problem 1
l = eval(input())
temp =''
for i in range(len(1)):
  for j in range(i+1,(len(1)-1):
    if l[j][0] !=l[i][-1]:
      temp =l[j]
      l[j] = l[j+1]
      l[j+1] = temp
print(l)  


#Problem 2
m=eval(input())
m=[str(i) for i m]
m.sort()
b=""
for i in range(len(m)-1):
  if m[i][0]==m[i+1][0] and len(m[i]<len(m[i+1]):
    b=m[i]
    m[i]=m[i+1]
    m[i+1]=b
for i in range(len(m)-1,-1,-1):
  c=c+m[i]
print(int(c))


    
